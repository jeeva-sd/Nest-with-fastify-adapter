{
    "name": "success-planning-server",
    "version": "3.0.0",
    "description": "",
    "author": "",
    "private": true,
    "license": "UNLICENSED",
    "scripts": {
        "cleanCommitHash": "rimraf src/constants/commit-hash.ts",
        "prebuild": "ts-node scripts/write-commit-hash.ts",
        "build": "nest build",
        "debug": "nest start --debug --watch",
        "predev": "rimraf node_modules/.prisma && npx prisma generate",
        "dev": "cross-env NODE_ENV=development nest start --watch",
        "preStaging": "rimraf node_modules/.prisma && npx prisma generate",
        "staging": "cross-env NODE_ENV=staging nest start",
        "prod": "cross-env NODE_ENV=production node dist/main",
        "format": "biome check --write --no-errors-on-unmatched",
        "deepFormat": "biome check --write --unsafe --no-errors-on-unmatched",
        "db:generate": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register scripts/run-prisma.ts generate && ts-node -r tsconfig-paths/register scripts/run-prisma.ts migrate dev --create-only",
        "db:update": "cross-env NODE_ENV=development ts-node scripts/run-prisma.ts migrate dev",
        "db:push": "cross-env NODE_ENV=development ts-node scripts/run-prisma.ts db push",
        "db:pushToStaging": "cross-env NODE_ENV=staging ts-node scripts/run-prisma.ts db push"
    },
    "dependencies": {
        "@fastify/compress": "^8.0.1",
        "@fastify/cookie": "^11.0.2",
        "@fastify/cors": "^11.0.0",
        "@fastify/csrf-protection": "^7.1.0",
        "@fastify/helmet": "^13.0.1",
        "@fastify/multipart": "^9.0.3",
        "@fastify/static": "^8.1.1",
        "@fastify/view": "^10.0.2",
        "@nestjs/common": "^11.0.11",
        "@nestjs/core": "^11.0.11",
        "@nestjs/jwt": "^11.0.0",
        "@nestjs/microservices": "^11.0.11",
        "@nestjs/passport": "^11.0.5",
        "@nestjs/platform-fastify": "^11.0.11",
        "@prisma/client": "^6.8.2",
        "amqp-connection-manager": "^4.1.14",
        "amqplib": "^0.10.5",
        "axios": "^1.8.1",
        "chalk": "^4.1.2",
        "cross-env": "^7.0.3",
        "crypto-js": "^4.2.0",
        "file-type": "^20.4.0",
        "handlebars": "^4.7.8",
        "nestjs-cls": "^5.4.3",
        "passport": "^0.7.0",
        "passport-custom": "^1.1.1",
        "passport-jwt": "^4.0.1",
        "passport-local": "^1.0.0",
        "reflect-metadata": "^0.2.2",
        "rimraf": "^6.0.1",
        "rxjs": "^7.8.2",
        "ts-loader": "^9.5.2",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "zod": "^3.25.20"
    },
    "devDependencies": {
        "@biomejs/biome": "^1.9.4",
        "@nestjs/cli": "^11.0.5",
        "@nestjs/schematics": "^11.0.1",
        "@nestjs/testing": "^11.0.11",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.13.8",
        "@types/supertest": "^6.0.2",
        "jest": "^29.7.0",
        "prisma": "^6.8.2",
        "source-map-support": "^0.5.21",
        "supertest": "^7.0.0",
        "ts-jest": "^29.2.6",
        "typescript": "^5.8.2"
    },
    "jest": {
        "moduleFileExtensions": ["js", "json", "ts"],
        "rootDir": "src",
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "collectCoverageFrom": ["**/*.(t|j)s"],
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
